<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display Names</title>
</head>
<body>

    <h2>List of Names:</h2>

    <div id="output"></div>

    <script>
        // Create an array with the given strings
        const names = ["john", "aaron", "edwin", "elizabeth"];

        // Initialize an empty string to hold the output
        let output = "";

        // Use a for loop to iterate through the array
        for (let i = 0; i < names.length; i++) {
            output += names[i] + "<br>";  // Add each name followed by a line break
        }

        // Display the output in the "output" div
        document.getElementById("output").innerHTML = output;
    </script>

</body>
</html>
